{
  "chrome": {
    "default": "120.0",
    "versions": {
      "120.0": {
        "image": "selenoid/chrome:120.0",
        "port": "4444",
        "path": "/",
        "volumes": [
          "//./pipe/docker_engine://./pipe/docker_engine",
          "/dev/shm:/dev/shm",
          "C:/selenoid/video:/data",
          "C:/selenoid/logs:/opt/selenoid/logs"
        ],
        "env": [
          "TZ=UTC",
          "SE_OPTS=--disable-dev-shm-usage --no-sandbox --disable-gpu",
          "SCREEN_RESOLUTION=1280x1024x24",
          "ENABLE_VNC=true"
        ],
        "shmSize": "2g",
        "tmpfs": {
          "/tmp": "size=512m"
        }
      }
    }
  }
}